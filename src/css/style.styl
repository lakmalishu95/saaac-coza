/* <><><><><><><><><><><><><><><><><> */
/* <><><><><><><><><><><><><><><><><> */
/* Header Section */
/* <><><><><><><><><><><><><><><><><> */
/* <><><><><><><><><><><><><><><><><> */

#section-header
  zb3()

#header-nav
  background red
  zb3()
  font-size 0.9em

#header-nav:hover
  zb5()
  opacity .9
  transition box-shadow 0.5s, opacity 5s

#header-topline
  background #fff
  zb1()
  a
    min-height 12px
    text-align center
    font-size 1em
    display block
    color grey
    text-decoration none
  p
    padding 10px
    background yellow
    margin 0

#header-topline:hover
  zb5()
  p
   //background red
   background #fff
   transition background 0.5s,box-shadow 0.5s

.topline-fixed
  position fixed
  top:0
  display block
  text-align center
  width 100%
  z-index 100
  zb5()

.topline-login
  font-size 0.5em !important
  text-align center
  color lightgray
  opacity 0.1
  text-decoration none

.topline-login:hover
  font-size 1.5em !important
  transition font-size 1s,background 1s,box-shadow 0.5s
  zb5()
  background yellow
  color #fff
  text-align center
  opacity 1
  text-decoration none

#header-nav-logo
  min-height 45px
  lost-column: 1/6 0 0
  background-image url("/images/header-nav-logo-border-right.png")
  background-repeat no-repeat
  background-position top right
  img
    margin-top 10px
    margin-bottom 10px
    width 100%
  img:hover
    opacity .8
  +below(750px)
    lost-column: 1/1 0 0
    lost-align: center
    min-height 90px
    background-image none
  +below(400px)
    lost-align: center
    lost-column: 1/1 0 0
    background-image none


#header-nav-links
  min-height 45px
  lost-column: 4/6 0 0
  background-image url("/images/header-links-background-tile.png")
  background-repeat repeat-x
  background-position top
  +below(965px)
    font-size 0.9em
  +below(820px)
    font-size 0.8em
  +below(750px)
    padding-top 3px
    lost-column: 1/1 0 0
  +below(500px)
    padding-top 5px
    padding-bottom 10px
    background-color grey
    background-image none
  ul
    list-style: none;
    text-align: center;
    padding: 0;
    margin: 0;

  li
    margin-top 10px
    display: inline-block;

  a
    margin-left: 5px;
    margin-right: 5px;
    border-style: solid;
    border-color: transparent
    border-width: 1px;
    border-radius: 5px;
    text-decoration: none;
    text-transform: uppercase;
    color: #fff;
    display: block;

  a:hover
    border-color white
    background white
    color: #000;
    zb4()
    transition background 1s

  a:active
    background blue !important
    color: #000;

.header-nav-links-link
  +below(340px)
    lost-column: 1/1 0 0
    a
      background white
  a
    background transparent
    padding-left 10px
    padding-right 10px
    margin-left 10px
    margin-right 10px
    display block
    text-decoration: none
    color: #000;

#header-nav-contact
  padding-top 20px
  min-height 45px
  lost-column: 1/6 0 0
  background-image url("/images/header-links-background-tile.png")
  background-repeat repeat-x
  +below(750px)
    padding-top 0px
    lost-column: 1/1 0 0
    background-image none
  a
    display block
    text-decoration: none
    color: #000;

#header-nav-contact-link
  text-align center
  a
    margin-left 20px
    margin-right 20px
    padding-top 10px
    padding-bottom 10px
    background yellow
    border-style: groove
    border-color: transparent
    border-width: 1px;
    border-radius: 5px;
    text-decoration: none;
    text-transform: uppercase;
    zb3()

#header-nav-contact-link:hover
  a
    zb5()
    background white
    transition background 1s

#header-hero-strip
  zb2()
  background-position left
  background-repeat repeat-x
  border-bottom solid 1px white
  border-top solid 1px white
  min-height 188px
  lost-align center
  text-align center

#header-hero-strip:hover
  background-position right
  zb5()
  transition box-shadow 0.5s, background-position 5s

#header-hero-title
  background grey
  border-radius 5px
  opacity 0.8
  margin 0
  padding 20px
  font-size 2em
  color #fff
  text-shadow 2px 2px #000;
  +below(450px)
    font-size 1.5em
  +below(300px)
    font-size 1em

