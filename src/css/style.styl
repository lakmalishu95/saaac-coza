@import 'imports/*'
@import '../../node_engine/*/ne-imports/style'

// Set the Breakpoints
bp = 960px

// Set the fonts later use // font-family font1

font1 = 'Asap', sans-serif

// The global styles

html
  font-size 1em
  color red
  font-family font1
  word-wrap: break-word

body
  background url("/images/saaac-tile-big.jpg") fixed
  padding 0
  margin 0
  display block


